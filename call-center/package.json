{
  "name": "call-center",
  "version": "1.0.0",
  "description": "Example Call Center application",
  "main": "index.js",
  "scripts": {
    "install:server": "cd server && npm install",
    "install:web-client": "cd web-client && npm install",
    "postinstall": "run-p install:*",
    "setup:server": "cd server && npm run setup",
    "setup": "run-p setup:*",
    "start:server": "cd server && npm start",
    "start:web-client": "cd web-client && npm start",
    "start": "run-p -l -r start:*"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/uuid": "^8.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.1.0"
  }
}
